{
  "name": "poketypes",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "yarn re:build && parcel build src/index.html --public-url .",
    "lint": "reanalyze",
    "test": "onchange --initial '{tests,src}/*.mjs' -- pta 'spec/*.test.mjs'",
    "re:analyze": "onchange --initial '{tests,src}/*.mjs' -- reanalyze",
    "ci": "yarn re:build && pta 'spec/*.test.mjs'",
    "re:build": "rescript || rescript build -with-deps",
    "re:start": "rescript build -w -with-deps"
  },
  "devDependencies": {
    "@dusty-phillips/rescript-zora": "^5.0.1",
    "@parcel/transformer-sass": "^2.12.0",
    "@rescript/core": "^1.6.0",
    "@rescript/react": "^0.13.0",
    "onchange": "^7.1.0",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "pta": "^1.3.0",
    "reanalyze": "^2.23.0",
    "rescript": "^11.1.4",
    "sass": "^1.79.5",
    "zora": "^6.0.0"
  },
  "dependencies": {
    "@glennsl/rescript-fetch": "^0.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "workbox": "^0.0.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "packageManager": "yarn@4.5.0"
}

{
  "name": "poketypes",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "yarn re:build && parcel build src/index.html --public-url .",
    "lint": "reanalyze",
    "test": "onchange --initial '{tests,src}/*.mjs' -- pta 'spec/*.test.mjs'",
    "ci": "rescript || rescript build -with-deps && pta 'spec/*.test.mjs'",
    "re:build": "rescript",
    "re:start": "rescript build -w -with-deps"
  },
  "devDependencies": {
    "@dusty-phillips/rescript-zora": "^3.0.2",
    "@rescript/react": "^0.11.0",
    "onchange": "^7.1.0",
    "parcel": "^2.8.2",
    "process": "^0.11.10",
    "pta": "^1.2.0",
    "reanalyze": "^2.23.0",
    "rescript": "^10.1.1",
    "sass": "^1.57.1",
    "zora": "^5.2.0"
  },
  "dependencies": {
    "@glennsl/rescript-fetch": "^0.1.4",
    "@ryyppy/rescript-promise": "^2.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "workbox": "^0.0.0",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "packageManager": "yarn@1.22.19"
}
